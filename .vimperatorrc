map j 3<C-e>
map k 3<C-y>

imap <C-h> <BS>
imap <C-d> <DEL>
imap <C-f> <RIGHT>
imap <C-b> <LEFT>
imap <C-p> <UP>
imap <C-n> <DOWN>
imap <C-j> <RETURN>

cmap <C-h> <BS>
cmap <C-d> <DEL>
cmap <C-f> <RIGHT>
cmap <C-b> <LEFT>

noremap h gT
noremap l gt

set complete=sl
set ignorecase

set hintchars=gjfkdlsa;h
